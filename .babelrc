{
  "env": {
    "development": {
      "presets": [
        "latest",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "add-module-exports", ["import", {
          "libraryName": "antd",
          "style": true
        }]
      ]
    },
    "test": {
      "presets": [
        "latest",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "add-module-exports",
        "istanbul", ["import", {
          "libraryName": "antd",
          "style": true
        }]
      ]
    },
    "production": {
      "presets": [
        "latest",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "add-module-exports", ["import", {
          "libraryName": "antd",
          "style": true
        }]
      ]
    }
  }
}
